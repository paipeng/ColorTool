      </ScrollPane>
      <AnchorPane maxWidth="300" prefHeight="1080.0" prefWidth="30.0">
         <children>
            <Label layoutX="9.0" layoutY="25.0" text="%backgroundImage" />
            <Button fx:id="backgroundImageButton" layoutX="130.0" layoutY="20.0" mnemonicParsing="false" text="%backgroundImage" />



            <Label layoutX="9.0" layoutY="80.0" text="labelPrintDpi" />
            <TextField fx:id="labelPrintDpiTextField" layoutX="130.0" layoutY="75.0" prefWidth="60.0" />


            <ColorPicker fx:id="backgroundColorPicker" layoutX="130.0" layoutY="110.0" />
            <Label layoutX="9.0" layoutY="115.0" text="%background" />


         </children>
      </AnchorPane>

   </SplitPane>
</VBox>
